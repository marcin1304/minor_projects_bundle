<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.StartController">
   <center>
      <SplitPane dividerPositions="0.5" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                 minWidth="-Infinity" prefHeight="428.0" prefWidth="600.0">
         <ListView fx:id="filesListView" prefHeight="200.0" prefWidth="259.0">
            <opaqueInsets>
               <Insets/>
            </opaqueInsets>
         </ListView>
         <Pane fx:id="filePane" maxHeight="1.7976931348623157E308" prefHeight="200.0" prefWidth="200.0"/>
      </SplitPane>
   </center>
   <bottom>
      <Label fx:id="loadSourceLabel" text="Label" BorderPane.alignment="CENTER" />
   </bottom>
   <top>
      <ScrollPane maxHeight="-Infinity" BorderPane.alignment="CENTER">
         <Label fx:id="currentDirectoryLabel" text="Label" wrapText="true"/>
      </ScrollPane>
   </top>
</BorderPane>
